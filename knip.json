{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    "common": {
      "entry": ["index.ts!","**/test-*.ts"],
      "project": "**/*.ts"
    },
    "service-a": {
      "entry": ["index.ts!", "**/test-*.ts"],
      "project": "**/*.ts"
    },
    "service-b": {
      "entry": ["src/index.ts!", "src/**/test-*.ts"],
      "project": "src/**/*.ts"
    }
  },
  "ignoreDependencies": ["c8", "multi-tape"]
}